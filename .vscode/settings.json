{
  "jest.pathToConfig": "jest.config.js",
  "editor.tabSize": 2,
  "files.associations": {
    "*.vue": "vue"
  },
  "search.useIgnoreFiles": false,
  "files.exclude": {},
  "search.exclude": {
    "**/.factor/**": true,
    "**/.ref": true,
    "*.lock": true,
    "*.log": true,
    "**/dist": true,
    "node_modules": true,
    "coverage": true,
    "**/CHANGELOG.md": true,
    "**/*.lcov": true,
    "**/junit": true
  },
  "editor.fontFamily": "Operator mono, Menlo, Monaco, 'Courier New', monospace",
  "eslint.validate": [
    {
      "language": "vue",
      "autoFix": true
    },
    {
      "language": "typescript",
      "autoFix": true
    },
    {
      "language": "html",
      "autoFix": true
    },
    {
      "language": "javascript",
      "autoFix": true
    },
    {
      "language": "json",
      "autoFix": true
    }
  ],
  "eslint.options": {
    "extensions": [".js", ".vue", ""]
  },
  "eslint.run": "onSave",
  "editor.formatOnSave": true,
  "markdownlint.config": {
    "default": true,
    "MD033": { "allowed_elements": ["a", "img", "p"] },
    "MD041": false,
    "MD036": false
  },
  "cSpell.enabledLanguageIds": [
    "asciidoc",
    "c",
    "cpp",
    "csharp",
    "css",
    "git-commit",
    "go",
    "handlebars",
