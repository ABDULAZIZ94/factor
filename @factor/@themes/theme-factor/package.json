{
  "name": "@factor/theme-factor",
  "version": "0.1.0",
  "main": "src/plugin.js",
  "author": "Fiction.com",
  "license": "GPL-3.0",
  "publishConfig": {
    "access": "public"
  },
  "factor": {
    "target": [
      "themes",
      "app",
      "build"
    ],
    "scope": "theme"
  },
  "scripts": {
    "develop": "NODE_ENV=development node -e 'require(`@factor/build-start`)()'",
    "devonly": "NODE_ENV=development node -e 'require(`@factor/build-development`)()' --no-deprecation",
    "dev": "NODE_ENV=development node start",
    "analyze": "NODE_ENV=production node start --analyze --build",
    "start": "NODE_ENV=production node start --build --serve --no-cache",
    "build": "NODE_ENV=production node start --build",
    "deploy": "yarn build && npx firebase use development && npx firebase deploy",
    "serve": "yarn build && npx firebase serve"
  },
  "gitHead": "4d2a7f165ca8fdcf56ad960fe04d210d5a2751c0"
}
