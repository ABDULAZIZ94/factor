{
  "name": "wrapper",
  "version": "0.0.1",
  "workspaces": [
    "@fiction/*",
    "@factor/*"
  ],
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "factor:dev": "yarn prep && yarn workspace @factor/theme-factor dev",
    "factor:start": "yarn prep && yarn workspace @factor/theme-factor start",
    "factor:prep": "yarn prep && yarn factor:dev",
    "app:dev": "yarn prep && yarn workspace @factor/app-starter dev",
    "app:start": "yarn prep && yarn workspace @factor/app-starter start",
    "app:prep": "yarn prep && yarn app:dev",
    "fiction:dev": "yarn prep && yarn workspace @fiction/www dev",
    "fiction:start": "yarn prep && yarn workspace @fiction/www start",
    "fiction:prep": "yarn prep && yarn fiction:dev",
    "prep": "git pull && cd @factor && git pull && cd ../ && cd @fiction && git pull && cd ../ && yarn install"
  },
  "devDependencies": {
    "lerna": "^3.13.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.12.0",
    "eslint-config-prettier": "^3.5.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-html": "^5.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-unicorn": "^7.0.0",
    "eslint-plugin-vue": "^5.1.0",
    "prettier": "^1.6.1",
    "prettier-eslint": "^8.8.2"
  }
}
